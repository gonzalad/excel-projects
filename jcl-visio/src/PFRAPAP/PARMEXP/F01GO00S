SELECT TCCONSOC,TCCONBEN,TCCOMCRC,TCCOMDBC,TCCOMCRC-TCCOMDBC                    
FROM PFRA.TBCOCOM                                                               
WHERE (TCCOMCRC <> TCCOMDBC)                                                    
  AND TCCONSOC = 100                                                            
ORDER BY TCCONBEN;                                                              
