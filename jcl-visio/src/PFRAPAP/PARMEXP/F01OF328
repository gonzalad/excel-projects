           TDPRCADRESCOUR     ,                                                 
           TDPRCINTERVDR      ,                                                 
           TDPRCOPTCPT        ,                                                 
           TDPRDCHGTOPTCPT    ,                                                 
           TDPRTNCEURO        ,                                                 
           TDPRCOPTASS        ,                                                 
           TDPRMAGIOSORIG     ,                                                 
           TDPRDDJOK1         ,                                                 
           TDPRDDJOK2         ,                                                 
           TDPRDDJOK3         ,                                                 
           TDPRMFMRB1         ,                                                 
           TDPRDFINANCABLE    ,                                                 
           TDPRMASMORIG       ,                                                 
           TDPRDDJOK          ,                                                 
           TDPRNAPERITEURFP   ,                                                 
           TDPRNCONVFP        ,                                                 
           TDPRNCONVSTYPEFP   ,                                                 
           TDPRNAVENANTFP     ,                                                 
           TDPRNAVENANTMDTFP  ,                                                 
           TDPRMFASSFP        ,                                                 
           TDPRCOPTFP         ,                                                 
           TDPRMFASSCOFP      ,                                                 
           TDPRCOPTCOFP       ,                                                 
           TDPRCSUIVISORTIE   ,                                                 
           TDPRDDPREM1        ,                                                 
           TDPRMVARAGIOORIG   ,                                                 
           TDPRMFRETORIG      ,                                                 
           TDPRMVARASSORIG                                                      
       FROM PFRA.TBDORAP, PFRA.TBDOPRN                                          
       WHERE                                                                    
         ((  TDPRNFOY      =  TDRPNFOYNEW   AND                                 
             TDPRNCONT     =  TDRPNCONTNEW  AND                                 
             TDPRNCARTE    =  TDRPNCARTENEW AND                                 
             TDPRNDT       =  TDRPNDTNEW    AND                                 
           ((TDRPNFOYNEW BETWEEN 4102059340 AND 4499999999 ) OR                 
             TDRPNFOYNEW     >=  8800000000  )                 )                
         OR                                                                     
          (  TDPRNFOY      =  TDRPNFOYOLD   AND                                 
             TDPRNCONT     =  TDRPNCONTOLD  AND                                 
             TDPRNCARTE    =  TDRPNCARTEOLD AND                                 
             TDPRNDT       =  TDRPNDTOLD    AND                                 
           ((TDRPNFOYOLD BETWEEN 4102059340 AND 4499999999 ) OR                 
             TDRPNFOYOLD     >=  8800000000  )                  )               
          )                                                                     
         AND                                                                    
             TDPRDMOD       <> '01.01.0001'                  AND                
             TDPRNOPERFIDEL = 14                             AND                
             TDPRNSOC     IN (100 , 114, 700, 714, 192)      AND                
        (TDRPDCREAT  >= ( SELECT MAX(DATE(TFPATIMESTAMP))                       
                          FROM PFRA.TBFIPAR                                     
                          WHERE TFPACNOMFIC = 'CGPE'                            
                          AND TFPALPROG = 'NEMEFFN' )        AND                
         ( (     CURRENT TIME >= '20.00.00'                                     
             AND CURRENT TIME <= '23.59.59'                                     
             AND TDRPDCREAT   <= CURRENT DATE                                   
           )                                                                    
           OR                                                                   
           (     CURRENT TIME >= '00.00.00'                                     
             AND CURRENT TIME <= '19.59.59'                                     
             AND TDRPDCREAT   <= (CURRENT DATE - 1 DAYS)                        
         ) ) )                                                                  
             ORDER BY TDRPNFOYNEW   ASC,                                        
                      TDRPNCONTNEW  ASC,                                        
                      TDRPNCARTENEW ASC,                                        
                      TDRPNDTNEW    DESC,                                       
                      TDRPDCREAT    ASC                                         
            WITH UR;                                                            

