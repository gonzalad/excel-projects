   FROM PFRA.TBBLOCN, PFRA.TBEXFOY                                              
   WHERE TBLONFOY = TEXFNFOY AND TBLONNAT IN (72520,72512)                      
   OPTIMIZE FOR 1 ROW                                                           
   UNLD DATA CONCURRENT FORMAT(DSNTIAUL) MODE(DB2SQL) OUTDDN(SYSREC78)          
   MAXTASKS 3                                                                   
