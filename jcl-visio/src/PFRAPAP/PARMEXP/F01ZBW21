  FROM PFRA.TBCLPRC                                                             
 WHERE TCPCNSOC = 517 AND TCPCNFOY NOT IN                                       
      (SELECT TDPRNFOY FROM PFRA.TBDOPRN WHERE TDPRNSOC = 517)                  
 GROUP BY TCPCNSOC                                                              
WITH UR ;                                                                       
